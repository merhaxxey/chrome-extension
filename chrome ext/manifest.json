{
    "manifest_version": 3,
    "name": "Auto Refresh Tab",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon1.png"
        }
    },
    "icons": {
        "128": "icon128.png"
    }
}