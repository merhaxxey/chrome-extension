{
    "manifest_version": 3,
    "name": "Auto Refresh Tab",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
      
    }
  }
  